# Alternative GNU Make workspace makefile autogenerated by Premake

ifndef config
  config=debug
endif

ifndef verbose
  SILENT = @
endif

ifeq ($(config),debug)
  GeomProject_config = debug

else ifeq ($(config),release)
  GeomProject_config = release

else
  $(error "invalid configuration $(config)")
endif

PROJECTS := GeomProject

.PHONY: all clean help $(PROJECTS) 

all: $(PROJECTS)

GeomProject:
ifneq (,$(GeomProject_config))
	@echo "==== Building GeomProject ($(GeomProject_config)) ===="
	@${MAKE} --no-print-directory -C . -f GeomProject.make config=$(GeomProject_config)
endif

clean:
	@${MAKE} --no-print-directory -C . -f GeomProject.make clean

help:
	@echo "Usage: make [config=name] [target]"
	@echo ""
	@echo "CONFIGURATIONS:"
	@echo "  debug"
	@echo "  release"
	@echo ""
	@echo "TARGETS:"
	@echo "   all (default)"
	@echo "   clean"
	@echo "   GeomProject"
	@echo ""
	@echo "For more information, see https://github.com/premake/premake-core/wiki"